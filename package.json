{
  "name": "event-badges-by-david",
  "version": "0.8.0",
  "description": "A Sketch plugin to make a large quantity of name tags for any kind of events",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "event-badges-by-david",
    "manifest": "src/manifest.json",
    "main": "event-badges-by-david.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/designforhuman/event-badges-by-david.git"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "davidlee <designforhuman@gmail.com>"
}
